{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":"off",
        "no-unused-vars":["error", { "argsIgnorePattern":"^_", "ignoreRestSiblings": true }],
        "no-throw-literal":"off",
        "prefer-default-export": "off",
        "consistent-return":"off"
    }
}
